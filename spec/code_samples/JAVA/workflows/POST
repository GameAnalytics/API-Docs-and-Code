  .body("{\"name\":\"<ADD STRING VALUE>\",\"application\":\"<ADD STRING VALUE>\",\"startsOn\":\"<ADD STRING VALUE>\",\"endsOn\":\"<ADD STRING VALUE>\",\"status\":\"ACTIVE\",\"triggerExpression\":\"<ADD STRING VALUE>\",\"abTest\":\"<ADD STRING VALUE>\",\"steps\":{}}")
  .asString();