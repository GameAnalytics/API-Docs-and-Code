import http.client

conn = http.client.HTTPConnection("preprod.api.datatiger.com")

headers = {
    'accept': "application/x-www-form-urlencoded",
    'content-type': "application/json"
    }

conn.request("GET", "/applications/%7Bapplication%7D/workflows?name=SOME_STRING_VALUE&version=SOME_STRING_VALUE&status=SOME_ARRAY_VALUE&fromTime=SOME_STRING_VALUE&endTime=SOME_STRING_VALUE&page=SOME_NUMBER_VALUE&perPage=SOME_NUMBER_VALUE&orderby=SOME_ARRAY_VALUE", headers=headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))