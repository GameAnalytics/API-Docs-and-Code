import http.client

conn = http.client.HTTPConnection("preprod.api.datatiger.com")

headers = { 'content-type': "application/json" }

conn.request("GET", "/applications/%7Bapplication%7D/workflows/%7Bworkflow%7D", headers=headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))