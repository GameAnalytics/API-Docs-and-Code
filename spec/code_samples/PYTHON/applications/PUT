import http.client

conn = http.client.HTTPConnection("preprod.api.datatiger.com")

headers = {
    'accept': "application/json",
    'content-type': "application/json"
    }

conn.request("PUT", "/applications/%7Bapplication%7D", headers=headers)

res = conn.getresponse()
data = res.read()

print(data.decode("utf-8"))