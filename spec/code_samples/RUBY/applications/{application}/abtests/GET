require 'uri'
require 'net/http'

url = URI("http://preprod.api.datatiger.com/applications/%7Bapplication%7D/abtests?name=SOME_STRING_VALUE&page=SOME_NUMBER_VALUE&perPage=SOME_NUMBER_VALUE&orderby=SOME_ARRAY_VALUE")

http = Net::HTTP.new(url.host, url.port)

request = Net::HTTP::Get.new(url)
request["content-type"] = 'application/json'

response = http.request(request)
puts response.read_body